{
    "$schema": "https://getcomposer.org/schema.json",
    "name": "trappyweb/scrape",
    "type": "project",
    "description": "Web scraper built with PHP for collecting structured data from websites.",
    "keywords": [
        "scrape"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.4",
        "chrome-php/chrome": "*"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    }
}
